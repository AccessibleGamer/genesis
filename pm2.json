{
  "apps": [
    {
      "name": "genesis-test",
      "script": "main.js",
      "exec_interpreter" : "node",
      "watch"            : ["genesis.json"],
      "log_date_format"  : "YYYY-MM-DD HH:mm Z",
      "env": {
      	"TOKEN": "<discord bot token>",
        "RAVEN_KEY": "<raven logging key>",
        "RAVEN_SECRET": "<raven logging secret>",
        "PREFIX": "++",
        "PORT": "8080",
        "MONGODB_URL": "<mongo db to store settings, etc, in>",
        "MD_LINE_END":"\n",
        "MD_BLOCK_END": "\n```",
        "MD_DOUBLE_RET": "　\r\n　　　\r\n",
        "MD_BOLD": "**",
        "MD_ITALIC": "*",
        "MD_UNDERLINE":"__",
        "MD_STRIKE":"~~",
        "MD_CODE_SINGLE":"`",
        "MD_CODE_BLOCK":"```haskell\n",
        "MD_CODE_BLOCK_END":"\n```",
        "MD_LINK_BEGIN":"\ ",
        "MD_LINK_MID":"\ :\ ",
        "MD_LINK_END":"\ ",
        "DISCORD_CARBON_TOKEN": "Not used yet",
        "DISCORD_BOTS_WEB_USER": "Not used yet",//not used yet
        "DISCORD_BOTS_WEB_TOKEN": "Not used yet",
        "OWNER":"<owner id for checking permissions",
        "LOG_LEVEL": "ERROR"
      }
    }
  ]
}